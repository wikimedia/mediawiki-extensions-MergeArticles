			],
			"styles": [
			],
			"dependencies": [
				"mediawiki.api",
				"mediawiki.Title",
				"oojs",
				"oojs-ui",
				"oojs-ui.styles.icons-editing-core"

			]
		},
		"ext.mergearticles.overview": {
			"scripts": [
				"widget/PageItem.js",
				"panel/Overview.js",
				"ext.mergearticles.overview.js"
			],
			"styles": [
				"ext.mergearticles.overview.less"
			],
			"dependencies": [
				"ext.mergearticles"
			],
			"messages": [
				"ma-target-exists-label",
