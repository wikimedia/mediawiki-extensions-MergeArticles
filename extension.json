		{
			"id": 1004,
			"constant": "NS_DRAFT",
			"name": "Draft"
		},
		{
			"id": 1005,
			"constant": "NS_DRAFT_TALK",
			"name": "Draft_talk"
		}
	],
	"config": {
		"MADraftFilePrefix": {
			"value": "Draft_"
		},
		"MAUseLineByLineDiff": {
			"value": false
		},
		"MAEnableBetaFeatures": {
			"value": false
		}
	},
	"manifest_version": 2
}
