			"scripts": [
				"panel/Review.js",
				"panel/Compare.js",
				"ext.mergearticles.compare.js"
			],
			"styles": [
				"ext.mergearticles.action.less"
			],
			"dependencies": [
				"ext.mergearticles"
			],
			"messages": [
				"ma-do-merge-label",
				"ma-diff-action-accept",
				"ma-diff-action-refuse",
				"ma-resolution-counter",
				"ma-same-block-label",
				"ma-diff-option-hide-identical-blocks-label",
				"ma-diff-option-accept-all-label",
				"ma-diff-accepted",
				"ma-diff-refused",
				"ma-diff-item-undo",
