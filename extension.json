				"ma-no-content-message",
				"ma-review-header",
				"ma-discard-draft",
				"ma-discard-draft-help",
				"ma-draft-discard-fail-header",
				"ma-draft-discard-success-header",
				"ma-draft-discard-success-text"
			]
